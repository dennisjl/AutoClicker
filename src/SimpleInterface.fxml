<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SimpleInterfaceController">
   <children>
      <Button fx:id="myButton" onMouseClicked="#click" layoutX="300.0" layoutY="132.0" mnemonicParsing="false" text="Confirm" />
      <TextField fx:id="myClicks" layoutX="25.0" layoutY="132.0" prefHeight="39.0" prefWidth="234.0" promptText="Enter number of clicks" />
      <TextField fx:id="myDelay" layoutX="25.0" layoutY="200.0" prefHeight="39.0" prefWidth="234.0" promptText="Enter delay between clicks" />
   </children>
</AnchorPane>
